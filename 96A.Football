#include <iostream>
#include <string>


using namespace std;

int main(){
    int ans = 0;
     string s;
     cin >> s;
     for (int i=0; i<s.length(); i++){
        if (s.substr(i,7) == "0000000" ||s.substr(i,7) == "1111111" )
            ans = 1;

     }
     if (ans) cout << "YES" << endl;
     else cout << "NO" << endl;

}
